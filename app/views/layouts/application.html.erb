  <script>
    $(function () {


      $('#menu').metisMenu({

        toggle: true // disable the auto collapse. Default: true.
      });
    });
  </script>
  <script>

    $('#menu').metisMenu({

// auto collapse.
      toggle: true,

// double tap to go
      doubleTapToGo: false,

// prevents or allows dropdowns' onclick events after expanding/collapsing.
      preventDefault: true,

// CSS classes
      activeClass: 'active',
      collapseClass: 'collapse',
      collapseInClass: 'in',
      collapsingClass: 'collapsing',

// callbacks
      onTransitionStart: false,
      onTransitionEnd: false

    });
  </script>
  <style>
    *{
      margin:0px auto;
    }
    .cent{
      /*text-align:center;*/

      font-size:12px;
      /*background-color: #ffffff;*/
    }
    .MainLayout td.cent
    {
      background-color: #ffffff;
      /*width:300px;*/
    }

    label {
      float: left;
      width: 10em;
      margin-right: 1em;
    }
    .submitbtn {
      float: left;
      width: 10em;
      margin-right: 1em;
    }

    /*label{*/
      /*width: 4em;*/
