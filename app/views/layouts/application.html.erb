      /*display: block*/
    /*}*/
    .arrow {
      float: right;
    }
    .glyphicon.arrow:before {
      content: "\e079";
    }
    .active > a > .glyphicon.arrow:before {
      content: "\e114";
    }
    .fa.arrow:before {
      content: "\f104";
    }
    .active > a > .fa.arrow:before {
      content: "\f107";
    }
    .plus-times {
      float: right;
    }
    .fa.plus-times:before {
      content: "\f067";
    }
    .active > a > .fa.plus-times {
      filter: progid:DXImageTransform.Microsoft.BasicImage(rotation=1);
      -webkit-transform: rotate(45deg);
      -moz-transform: rotate(45deg);
      -ms-transform: rotate(45deg);
      -o-transform: rotate(45deg);
      transform: rotate(45deg);
    }
    .plus-minus {
      float: right;
    }
    .fa.plus-minus:before {
      content: "\f067";
    }
    .active > a > .fa.plus-minus:before {
      content: "\f068";
    }
    #footer {
      position: fixed;
      bottom: 0;
      width: 100%;
    }

    /***** TABLES *****/
    form
    {
      margin: 0;
      padding: 0;
    }

    table.MainLayout
    {
      width: 98%;
      border: 1px solid #dddddd;
      border-collapse:collapse;
      border-spacing: 0;
      border: 0px;

    }

    table.MainLayout td
    {
      border: 1px solid #dddddd;
      text-align: left;
      padding: 5px 0 5px 0;
      border: 0px;
    }

    table.MainLayout td
    {
      font-size: 13px;
      color: #101010;
      font-weight: 500;
      padding-left: 0px;
      text-align: left;
      /*background-color: #fdfbec;*/
      border: 0px;
    }
    table.MainLayout tr th
    {
      /*cursor: pointer;*/
      background: url('/assets/thBg2.jpg');
      background-repeat: repeat-x;
      height: 48px;
      padding: 0 10px 0 10px;
      margin: 0;
      font-size: 14px;
      font-weight: bold;
      color: #4c4c4c;
      line-height: 15px;
      border: 1px solid #dddddd;

    }
    th:hover
    {
      /*cursor: pointer;*/
      /*color: #598d9a;*/
      /*cursor: pointer;*/
    }

    table.MainLayout tr:hover,
    table.MainLayout tr.selected
    {
      /*cursor: pointer;*/
      /*background-color: #fdfbec;*/

      -webkit-transition: background linear 0.25s, box-shadow linear 0.2s;
      -moz-transition: background linear 0.25s, box-shadow linear 0.2s;
      -o-transition: background linear 0.25s, box-shadow linear 0.2s;
    }

    .tableContentCell
    {
      padding: 0;
      margin: 0;
    }

    .tableContentBg
    {
      border: 4px solid #999999;
    }

    .anyContentBg
    {
      border-top: 3px solid #c9c9c9;
      border-bottom: 1px solid #dddddd;
      border-right: 1px solid #dddddd;
      border-left: 1px solid #dddddd;
    }

  </style>

